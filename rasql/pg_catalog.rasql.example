{
	"synopsis":		"PostgreSQL 9.6 System Catalog Schema",
	"http-listen":		":8080",
	"rest-path-prefix":	"/pg_catalog",

	"sql-queries": {
			"pg_class":		{
				"source-path":	"pg_class.sql"
			},

			"pg_class-by-nspname":	{
				"source-path":	"pg_class-by-nspname.sql"
			}
	},

	"http-GET-args": {
		"q": {
			"matches": "^.{0,255}",
			"required": true
		},
		"lim": {
			"default": "10",
			"matches": "^(?:10|100|1000|10000)$",
			"required": true
		}
	}
}
